{
    "manifest_version": 3,
    "name": "No YouTube",
    "version": "1.0",

    "description": "Browser extension for help to stop watch YouTube 24/7.",

    "icons": {
        "48": "icons/icon-48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["main.js"]
        }
    ]
}
